{
  "name": "CCASCADES",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "copy": "cp -r data geojson img lib dist/ && cp -r src/*.css src/i18n/ dist/src",
    "build": "npx rollup -c && npm run uglify && npm run copy && npm run html",
    "uglify": "npx uglifyjs-folder dist --each --output dist",
    "html": "cp *.html dist && replace-in-file --configFile=replace-config.js --verbose",
    "test": "npm run test-lint",
    "test-lint": "npx eslint .",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "author": "",
  "license": "AGPL",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.7.0",
    "eslint-config-google": "^0.11.0",
    "replace-in-file": "^3.4.3",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "uglifyjs-folder": "^1.5.1"
  },
  "dependencies": {
    "a-color-picker": "^1.1.7",
    "details-element-polyfill": "^2.3.1",
    "details-polyfill": "^1.1.0",
    "http-server": "^0.11.1"
  }
}
